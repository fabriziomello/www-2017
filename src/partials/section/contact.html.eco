
<!-- CONTACT -->
<section class="contact" id="contact">
  <h2 class="section-title"><%= @t('sections.contact.title')%></h2>
  <p>
    <%- @t('sections.contact.message')%>
  </p>
  <p class="icon-links">
    <% for social in @sections.contact.social: %>
      <a target="_blank" href="<%= social.url %>" title="<%= social.name %>" class="fa <%= social.icon %>"></a>
    <% end %>
  </p>
</section>
<!-- / CONTACT -->
